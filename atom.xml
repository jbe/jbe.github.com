
--
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Jostein Berre Eliassen</title>
	<link href="http://jostein.be/atom.xml" rel="self"/>
	<link href="http://jostein.be/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://jostein.be/</id>
	<author>
		<name>Jostein Berre Eliassen</name>
		<email>http://jostein.be</email>
	</author>

	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://jostein.be{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://jostein.be{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}

</feed>
